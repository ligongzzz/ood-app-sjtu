---
batch_connect:
  template: vnc
script:
  <%- if instance_size == "40core" -%>
  queue_name: "cpu"
  <%- end -%>
  native:
    <%- if instance_size == "1core" -%>
    - "-J studio_blender_1core"
    - "-n 1"
    <%- end -%>
    <%- if instance_size == "16core" -%>
    - "-J studio_blender_16core"
    - "-n 16"
    <%- end -%>
    <%- if instance_size == "40core" -%>
    - "-J studio_blender_40core"
    - "--exclusive"
    <%- end -%>

